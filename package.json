    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-toastify": "^10.0.5",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.5.0",
    "remix-auth-google": "^2.0.0",
    "remix-toast": "^1.2.0",
    "remix-utils": "^7.6.0",
    "zod": "^3.23.8"
